{
	"include": ["./src"],
	"exclude": ["src/**/*.test.ts", "src/test/**/*"],
	"compilerOptions": {
		"sourceMap": true,
		"rootDir": "./src",
		"outDir": "./dist",
		"declaration": true,
		"module":"nodenext",
		"target": "esnext",
		"moduleResolution": "nodenext",
		"skipLibCheck": true,
		"strict": true
	}
}